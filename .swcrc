{
    "$schema": "https://json.schemastore.org/swcrc",
    "sourceMaps": true,
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "decorators": true,
        "dynamicImport": true
      },
      "baseUrl": "./",
      "paths": {
        "@src/*": ["src/*"],
        "@core/*": ["src/core/*"],
        "@logger/*": ["src/logger/*"]
      },
      "target": "es2022"
    },
    "module": {
      "type": "commonjs"
    },
    "minify": false
}